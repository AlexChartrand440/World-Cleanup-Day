{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "flowtype"
  ],
  "rules": {
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/prefer-default-export":0,
    "react/jsx-filename-extension": 0,
    "no-undef": 0,
    "no-console": 0,
    "arrow-parens": 0,
    "no-unused-expressions": "warn",
    "max-len": [1, 80]
  }
}